extends ../layout 

block content
  a(href='/bikes') Volver 

  br
  br
  br

  h1  Nueva Bicicleta 

  br
  br

  form(action='/bikes/create' method='post')
    label(for='id') ID: 
    input#id(type='text' name='id')
    br
    label(for='color') Color:
    input#color(type='text' name='color')
    br
    label(for='model') Modelo: 
    input#model(type='text' name='model')
    br
    label(for='lat') Latitud: 
    input#model(type='text' name='lat')
    br
    label(for='lng') Longitud: 
    input#model(type='text' name='lng')
    br
    br
    button(type='submit' value='OK') Confirmar
    